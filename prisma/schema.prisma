generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id          String   @id @default(uuid())
  clientName  String
  email       String   
  phoneNumber String
  brandName   String?
  agentName   String?
  createdAt   DateTime @default(now())
}
